<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.lcb.study.vipcourseuitest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".navigation.NavigationActivity">
            <!-- 为Activity设置<nav-graph/>标签 -->
            <nav-graph android:value="@navigation/nav_graph" />
        </activity>
        <activity android:name=".viewmodel.DatabindingActivity">
        </activity>
        <activity android:name=".viewmodel.ViewModelActivity">
        </activity>
        <activity android:name=".livedata.TwoActivity">
        </activity>
        <activity android:name=".livedata.LiveDataActivity">
        </activity>
        <activity android:name=".animation.AnimationActivity">
        </activity>
        <activity android:name=".motionevent.EventDispatchActivity">
        </activity>
        <activity android:name=".recyclerview.RecyclerViewActivity">
        </activity>
        <activity android:name=".flowlayout.FlowLayoutActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>